{
	"name": "life-grid",
	"version": "1.0.0",
	"description": "Visualize your entire life as an interactive grid, where each dot represents a day. Highlight notes, life periods, and milestones to reflect on your past, track your present, and plan your future.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"build:watch": "node esbuild.config.mjs watch"
	},
	"keywords": [
		"obsidian-plugin",
		"life-tracking",
		"visualization",
		"daily-notes",
		"time-management"
	],
	"author": "F. Javier R. Donado",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/mrdonado/obsidian-life-grid.git"
	},
	"bugs": {
		"url": "https://github.com/mrdonado/obsidian-life-grid/issues"
	},
	"homepage": "https://github.com/mrdonado/obsidian-life-grid#readme",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "^1.8.7",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {}
}